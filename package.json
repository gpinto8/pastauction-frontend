{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "vite-vuetify-ts-starter",
  "description": "Vue3 Vuetify TypeScript Startar project for Vite.",
  "version": "1.5.10",
  "license": "MIT",
  "type": "module",
  "private": true,
  "author": {
    "name": "Logue",
    "email": "logue@hotmail.co.jp",
    "url": "https://logue.dev/"
  },
  "homepage": "https://github.com/logue/vite-vuetify-ts-starter",
  "repository": {
    "type": "git",
    "url": "git@github.com:logue/vite-vuetify-ts-starter.git"
  },
  "bugs": {
    "url": "https://github.com/logue/vite-vuetify-ts-starter/issues"
  },
  "engines": {
    "node": ">=18.16.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite",
    "clean": "rimraf ./node_modules/.vite",
    "build": "run-p type-check build-only",
    "build:analyze": "vite build --mode analyze",
    "build:clean": "rimraf dist",
    "preview": "vite preview",
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . -w -u",
    "lint:style": "stylelint \"./**/*.{css,sass,scss,htm,html,vue}\" --fix --cache-location ./node_modules/.vite/vite-plugin-stylelint && prettier \"./**/*.{css,sass,scss,htm,html,vue}\" -w -u",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "prepare": "husky install",
    "vue-tsc": "vue-tsc"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.16.8",
    "@heroicons/vue": "^2.1.5",
    "@mdi/font": "^7.4.47",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
    "@vuepic/vue-datepicker": "^6.1.0",
    "axios": "^1.7.7",
    "chart.js": "^4.4.4",
    "commonjs": "^0.0.1",
    "core-js": "^3.38.1",
    "deep-clone": "^4.0.0",
    "pinia": "^2.2.2",
    "pinia-plugin-persistedstate": "^3.2.3",
    "rxjs": "^7.8.1",
    "swiper": "^11.1.14",
    "ts-pattern": "^5.3.1",
    "vee-validate": "^4.13.2",
    "vue": "^3.5.8",
    "vue-chartjs": "^5.3.1",
    "vue-fullpage.js": "^0.2.19",
    "vue-gallery-slideshow": "^1.5.2",
    "vue-router": "^4.4.5",
    "vuetify": "^3.7.2",
    "vuex": "^4.1.0",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@babel/types": "^7.25.6",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rushstack/eslint-patch": "^1.10.4",
    "@tsconfig/node-lts": "^18.12.5",
    "@types/google.maps": "^3.58.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.16.6",
    "@types/webfontloader": "^1.6.38",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vitest/coverage-c8": "^0.33.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.20",
    "eslint": "8.0.0",
    "eslint-config-standard-with-typescript": "latest",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-vue": "^9.28.0",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "eslint-plugin-vuetify": "^2.4.0",
    "eslint-plugin-yaml": "^0.5.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.3.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.47",
    "postcss-html": "^1.7.0",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "rimraf": "^5.0.10",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.79.3",
    "stylelint": "^15.11.0",
    "stylelint-config-recommended-scss": "^12.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-order": "^6.0.4",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.6.2",
    "vite": "^4.5.5",
    "vite-plugin-checker": "0.6.1",
    "vite-plugin-vuetify": "^1.0.2",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.27"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,yaml,yml,vue,htm,html,md}": "eslint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*.{css,sass,scss,vue,htm,html}": "stylelint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-stylelint",
    "*": "prettier -w -u"
  },
  "resolutions": {
    "http-cache-semantics": "^4.1.1",
    "json5": "^2.2.3",
    "semver": "^7.5.2",
    "yaml": "^2.2.2"
  },
  "stackblitz": {
    "startCommand": "pnpm run test:unit"
  }
}
